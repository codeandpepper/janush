{
  "name": "@codeandpepper/janush",
  "version": "0.1.0",
  "description": "A blank schematics",
  "engines": {
    "node": "14.x.x"
  },
  "scripts": {
    "build": "tsc && tsc-alias --silent",
    "test": "npm run build && jasmine schematics/**/*_spec.js",
    "create": "npm run build && schematics .:application",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json}\""
  },
  "keywords": [
    "schematics"
  ],
  "main": "cli/index.js",
  "bin": {
    "@codeandpepper/janush": "bin/janush",
    "janush": "bin/janush"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "schematics": "@codeandpepper/janush/schematics/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.2.4",
    "@angular-devkit/schematics": "^12.2.4",
    "@schematics/angular": "^12.2.4",
    "arg": "^5.0.1",
    "esm": "^3.2.25",
    "typescript": "~4.3.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.8.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.10.1",
    "@types/prettier": "^2.3.2",
    "jasmine": "^3.5.0",
    "prettier": "^2.3.2",
    "tsc-alias": "^1.3.9"
  }
}
