{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "cloud.template": {
      "aliases": ["cloud"],
      "description": "A blank cloud application generated by aws-cdk v2.",
      "factory": "./template/index#cloudTemplateGenerator",
      "schema": "./template/schema.json"
    },
    "cloud.janush": {
      "description": "A basic janush cloud codebase.",
      "factory": "./janush/index#cloudJanushGenerator",
      "schema": "./janush/schema.json",
      "hidden": true
    },
    "cloud.authorization": {
      "description": "AWS cognito user pool and identity pool.",
      "factory": "./authorization/index#cloudAuthorizationGenerator",
      "schema": "./authorization/schema.json"
    },
    "cloud.authorization.emails": {
      "description": "AWS cognito custom message emails.",
      "factory": "./authorization-emails/index#cloudAuthorizationEmailsGenerator",
      "schema": "./authorization-emails/schema.json"
    }
  }
}
